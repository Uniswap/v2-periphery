{
  "name": "dxswap-periphery",
  "version": "1.0.0",
  "description": "Peripheral smart contracts for interacting with DXswap core contracts",
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/levelkdev/dxswap-periphery"
  },
  "files": [
    "build",
    "contracts"
  ],
  "dependencies": {
    "@uniswap/lib": "1.1.1",
    "dxswap-core": "git://github.com/levelkdev/dxswap-core.git#develop"
  },
  "devDependencies": {
    "@types/chai": "^4.2.6",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "ethereum-waffle": "^2.4.1",
    "ethereumjs-util": "^6.2.0",
    "mocha": "^6.2.2",
    "ncp": "^2.0.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "solc": "0.6.6",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3",
    "truffle-flattener": "^1.4.4"
  },
  "scripts": {
    "lint": "yarn prettier ./test/*.ts --check",
    "lint:fix": "yarn prettier ./test/*.ts --write",
    "clean": "rimraf ./build/",
    "copy-v1-artifacts": "ncp ./buildV1 ./build",
    "precompile": "yarn clean",
    "compile": "waffle .waffle.json",
    "postcompile": "yarn copy-v1-artifacts",
    "pretest": "yarn compile",
    "test": "mocha",
    "prepublishOnly": "yarn test",
    "flattener": "./scripts/flattener.sh"
  },
  "license": "GPL-3.0-or-later"
}
